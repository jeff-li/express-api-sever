{
  "name": "express-api-sever",
  "version": "2.0.0",
  "private": true,
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "postgres"
  ],
  "scripts": {
    "start": "npm run migrate && ./bin/www",
    "dev": "npm run migrate && nodemon ./bin/www",
    "test": "env NODE_ENV=test nyc --reporter text mocha --recursive --check-leaks --exit",
    "test-watch": "env NODE_ENV=test mocha -w --exit",
    "report-coverage": "nyc report --reporter=lcov --reporter=text",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "lint": "eslint . --ext .js --color"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "~4.17.2",
    "helmet": "^4.6.0",
    "http-errors": "~2.0.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "sequelize": "^6.12.0",
    "sequelize-cli": "^6.3.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "2.5.1",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "swagger-ui-express": "^4.3.0"
  }
}
