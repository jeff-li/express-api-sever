{
  "name": "express-api-starter-pack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run migrate && ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "env NODE_ENV=test nyc --reporter text mocha --recursive --check-leaks --exit",
    "test-watch": "env NODE_ENV=test mocha -w --exit",
    "report-coverage": "nyc report --reporter=lcov --reporter=text",
    "migrate": "sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "helmet": "^3.22.0",
    "http-errors": "~1.7.3",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0"
  }
}
